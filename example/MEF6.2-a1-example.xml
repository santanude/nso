<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <mef-global xmlns="urn:mef:yang:mef-global">
    <sls-profiles>
      <profile>
        <id>SLS-EPL1</id>
        <time-interval>25920000</time-interval>
        <pm-cos-name-entries>
          <pm-cos-name-entry>
            <cos-name>krypton</cos-name>
            <delta-t>1</delta-t>
            <threshold-c>50</threshold-c>
            <consecutive-interval-n>10</consecutive-interval-n>
            <pm-entries>
              <pm-entry>
                <id>fd</id>
                <one-way-frame-delay-pm>
                  <percentile>99.9</percentile>
                  <objective>10</objective>
                </one-way-frame-delay-pm>
              </pm-entry>
              <pm-entry>
                <id>ifdv</id>
                <one-way-inter-frame-delay-variation-pm>
                  <percentile>99.9</percentile>
                  <frame-pair-separation>1</frame-pair-separation>
                  <objective>1</objective>
                </one-way-inter-frame-delay-variation-pm>
              </pm-entry>
              <pm-entry>
                <id>flr</id>
                <one-way-frame-loss-ratio-pm>
                  <objective>0.01</objective>
                </one-way-frame-loss-ratio-pm>
              </pm-entry>
              <pm-entry>
                <id>availability</id>
                <one-way-availability-pm>
                  <objective>99.999</objective>
                </one-way-availability-pm>
              </pm-entry>
            </pm-entries>
          </pm-cos-name-entry>
          <pm-cos-name-entry>
            <cos-name>argon</cos-name>
            <delta-t>1</delta-t>
            <threshold-c>75</threshold-c>
            <consecutive-interval-n>10</consecutive-interval-n>
            <pm-entries>
              <pm-entry>
                <id>fd</id>
                <one-way-frame-delay-pm>
                  <percentile>99</percentile>
                  <objective>20</objective>
                </one-way-frame-delay-pm>
              </pm-entry>
              <pm-entry>
                <id>flr</id>
                <one-way-frame-loss-ratio-pm>
                  <objective>0.2</objective>
                </one-way-frame-loss-ratio-pm>
              </pm-entry>
              <pm-entry>
                <id>availability</id>
                <one-way-availability-pm>
                  <objective>99.9</objective>
                </one-way-availability-pm>
              </pm-entry>
            </pm-entries>
          </pm-cos-name-entry>
          <pm-cos-name-entry>
            <cos-name>neon</cos-name>
            <delta-t>1</delta-t>
            <threshold-c>100</threshold-c>
            <consecutive-interval-n>10</consecutive-interval-n>
            <pm-entries>
              <pm-entry>
                <id>fd</id>
                <one-way-frame-delay-pm>
                  <percentile>95</percentile>
                  <objective>30</objective>
                </one-way-frame-delay-pm>
              </pm-entry>
              <pm-entry>
                <id>flr</id>
                <one-way-frame-loss-ratio-pm>
                  <objective>0.3</objective>
                </one-way-frame-loss-ratio-pm>
              </pm-entry>
              <pm-entry>
                <id>availability</id>
                <one-way-availability-pm>
                  <objective>99</objective>
                </one-way-availability-pm>
              </pm-entry>
            </pm-entries>
          </pm-cos-name-entry>
        </pm-cos-name-entries>
      </profile>
    </sls-profiles>
    <cos-names-profiles>
      <profile>
        <name>krypton</name>
      </profile>
      <profile>
        <name>argon</name>
      </profile>	
      <profile>
        <name>neon</name>
      </profile>	
    </cos-names-profiles>
    <l2cp-cos-map-profiles>
      <profile>
        <id>EPL1_L2CP</id>
        <map-entries>
          <map-entry>
            <protocol>ethertype</protocol>
	    <!-- protocol-id is a uint16 value, so 0x8100 would -->
            <protocol-id>33024</protocol-id>
            <cos-name>krypton</cos-name>
          </map-entry>
        </map-entries>
      </profile>
    </l2cp-cos-map-profiles>
    <color-mapping-profiles>
      <profile>
        <id>EPL1COLOR</id>
        <color-evc>
          <evc-color-name>yellow</evc-color-name>          
        </color-evc>
      </profile>
    </color-mapping-profiles>
    <eec-profiles>
      <profile>
	<id>EPL1_EECP</id>
	<eec-dscp>
	  <default-ipv4-eec-name>EECP_IPV4_5</default-ipv4-eec-name>
	  <default-ipv6-eec-name>EECP_IPV6_5</default-ipv6-eec-name>
	  <ipv4-dscp>
	    <dscp-value>5</dscp-value>
	    <eec-name>EECP_5</eec-name>
	  </ipv4-dscp>
	</eec-dscp>
      </profile>
    </eec-profiles>
    <cos-profiles>
      <profile>
        <id>EPL1L2CPCoS</id>
        <l2cp-cos-id>EPL1_L2CP</l2cp-cos-id>
        <cos-evc>
          <evc-cos-name>krypton</evc-cos-name>
        </cos-evc>
      </profile>
      <profile>
        <id>EPL1CoS</id>
        <cos-evc>
          <evc-cos-name>krypton</evc-cos-name>
        </cos-evc>
      </profile>
      <profile>
        <id>EPL2CoS</id>
        <cos-pcp>
          <pcp>
            <pcp-value>7</pcp-value>
            <discard></discard>
          </pcp>
          <pcp>
            <pcp-value>6</pcp-value>
            <discard></discard>
          </pcp>
          <pcp>
            <pcp-value>5</pcp-value>
            <cos-name>krypton</cos-name>
          </pcp>
          <pcp>
            <pcp-value>4</pcp-value>
            <discard></discard>
          </pcp>
          <pcp>
            <pcp-value>3</pcp-value>
            <cos-name>argon</cos-name>
          </pcp>
          <pcp>
            <pcp-value>2</pcp-value>
            <discard></discard>
          </pcp>
          <pcp>
            <pcp-value>1</pcp-value>
            <cos-name>neon</cos-name>
          </pcp>
          <pcp>
            <pcp-value>0</pcp-value>
            <discard></discard>
          </pcp>
        </cos-pcp>
      </profile>
    </cos-profiles>
  </mef-global>
  <mef-interfaces xmlns="urn:mef:yang:mef-legato-interfaces">
    <carrier-ethernet>
      <subscriber-interfaces>
        <uni>
          <uni-id>UNI1</uni-id>
          <physical-layers>
            <links>
              <link>
                <link-number>1</link-number>
                <ieee8023-phy xmlns:mef-types="urn:mef:yang:mef-types">
                  mef-types:ieee8023-1000BASE-SX
		</ieee8023-phy>
                <sync-mode>false</sync-mode>
              </link>
            </links>
          </physical-layers>
          <link-aggregation>none</link-aggregation>
          <max-frame-size>1522</max-frame-size>
          <service-multiplexing-enabled>false</service-multiplexing-enabled>
          <!--<bundling-enabled>false</bundling-enabled>
          <all-to-one-bundling-enabled>true</all-to-one-bundling-enabled>-->
          <default-ce-vlan-id>1</default-ce-vlan-id>
          <max-num-of-evcs>1</max-num-of-evcs>
          <token-share-enabled>false</token-share-enabled>
          <link-oam-enabled>false</link-oam-enabled>
          <uni-meg-enabled>false</uni-meg-enabled>
          <elmi-enabled>false</elmi-enabled>
          <l2cp-address-set>ctb2</l2cp-address-set>
        </uni>
	<uni>
          <uni-id>UNI2</uni-id>
          <physical-layers>
            <links>
              <link>
                <link-number>1</link-number>
                <ieee8023-phy xmlns:mef-types="urn:mef:yang:mef-types">
                  mef-types:ieee8023-1000BASE-SX
		</ieee8023-phy>
                <sync-mode>false</sync-mode>
              </link>
            </links>
          </physical-layers>
          <link-aggregation>none</link-aggregation>
          <max-frame-size>1522</max-frame-size>
          <service-multiplexing-enabled>false</service-multiplexing-enabled>
          <!--<bundling-enabled>false</bundling-enabled>
          <all-to-one-bundling-enabled>true</all-to-one-bundling-enabled>-->
          <default-ce-vlan-id>1</default-ce-vlan-id>
          <max-num-of-evcs>1</max-num-of-evcs>
          <token-share-enabled>false</token-share-enabled>
          <link-oam-enabled>false</link-oam-enabled>
          <uni-meg-enabled>false</uni-meg-enabled>
          <elmi-enabled>false</elmi-enabled>
          <l2cp-address-set>ctb2</l2cp-address-set>
        </uni>
      </subscriber-interfaces>
    </carrier-ethernet>
  </mef-interfaces>
  <mef-services xmlns="urn:mef:yang:mef-legato-services">
    <carrier-ethernet>
      <subscriber-services>
        <evc>
          <evc-id>EPL1</evc-id>
          <cos-names>
            <cos-name>
              <name>krypton</name>
            </cos-name>
          </cos-names>
          <end-points>
            <end-point>
              <uni-id>UNI1</uni-id>
              <role>root</role>
	      <color-identifier>EPL1COLOR</color-identifier>
	      <cos-identifier>EPL1CoS</cos-identifier>
	      <eec-identifier>EPL1_EECP</eec-identifier>
             <!-- <source-mac-address-limit>100</source-mac-address-limit>
	      <source-mac-address-limit-time-interval>2000</source-mac-address-limit-time-interval>-->
              <subscriber-meg-mip-enabled>false</subscriber-meg-mip-enabled>
              <ce-vlans>
		<ce-vlan>1</ce-vlan>
                <!--<ce-vlan>1</ce-vlan>
                <ce-vlan>4094</ce-vlan>-->
              </ce-vlans>
              <test-meg-enabled>false</test-meg-enabled>
            </end-point>
	    <end-point>
              <uni-id>UNI2</uni-id>
              <role>root</role>
	      <color-identifier>EPL1COLOR</color-identifier>
	      <cos-identifier>EPL1CoS</cos-identifier>
	      <eec-identifier>EPL1_EECP</eec-identifier>
             <!-- <source-mac-address-limit>100</source-mac-address-limit>
	      <source-mac-address-limit-time-interval>2000</source-mac-address-limit-time-interval>-->
              <subscriber-meg-mip-enabled>false</subscriber-meg-mip-enabled>
              <ce-vlans>
		<ce-vlan>1</ce-vlan>
                <!--<ce-vlan>1</ce-vlan>
                <ce-vlan>4094</ce-vlan>-->
              </ce-vlans>
              <test-meg-enabled>false</test-meg-enabled>
            </end-point>
          </end-points>
          <carrier-ethernet-sls>
            <sls-id>SLS-EPL1</sls-id>
            <start-time>2015-05-19T16:39:57-08:00</start-time>
            <cos-entries>
              <cos-entry>
                <cos-name>krypton</cos-name>
                <pm-entries>
                  <pm-entry>
                    <pm-entry-id>fd</pm-entry-id>
                    <end-point-pairs>
                      <sls-uni-exclusions>
                      </sls-uni-exclusions>
                    </end-point-pairs>
                  </pm-entry>
                  <pm-entry>
                    <pm-entry-id>ifdv</pm-entry-id>
                    <end-point-pairs>
                      <sls-uni-exclusions>
                      </sls-uni-exclusions>
                    </end-point-pairs>
                  </pm-entry>
                  <pm-entry>
                    <pm-entry-id>flr</pm-entry-id>
                    <end-point-pairs>
                      <sls-uni-exclusions>
                      </sls-uni-exclusions>
                    </end-point-pairs>
                  </pm-entry>
                  <pm-entry>
                    <pm-entry-id>availability</pm-entry-id>
                    <end-point-pairs>
                      <sls-uni-exclusions>
                      </sls-uni-exclusions>
                    </end-point-pairs>
                  </pm-entry>
                </pm-entries>
              </cos-entry>
            </cos-entries>
          </carrier-ethernet-sls>
          <connection-type>point-to-point</connection-type>
          <max-frame-size>1522</max-frame-size>
          <max-num-of-evc-end-point>2</max-num-of-evc-end-point>
          <ce-vlan-id-preservation>true</ce-vlan-id-preservation>
          <ce-vlan-pcp-preservation>true</ce-vlan-pcp-preservation>
          <unicast-frame-delivery>unconditional</unicast-frame-delivery>
          <multicast-frame-delivery>unconditional</multicast-frame-delivery>
          <broadcast-frame-delivery>unconditional</broadcast-frame-delivery>
          <svc-type>epl</svc-type>
        </evc>
      </subscriber-services>
    </carrier-ethernet>
  </mef-services>
</config>
